<xml>
    <AperturaExpedienteElectronico>
        <!--Constitución de la sociedad mercantil-->
        <FedatarioPublico>
            <rfcFe></rfcFe>
            <curpFe></curpFe>
            <!--Cuando el valor es verdadero indica que el instrumento lo genera un Notario Público, en caso de que sea falso indica que es un Corredor-->
            <isNotario></isNotario>
            <nombreFe></nombreFe>
            <primerApellidoFe></primerApellidoFe>
            <segundoApellidoFe></segundoApellidoFe>
            <entidadFederativaFedatario></entidadFederativaFedatario>
            <municipioFedatario></municipioFedatario>
            <!--Notaria o Correduria-->
            <numeroOficina></numeroOficina>
        </FedatarioPublico>
        <personalFedatario>
            <RFC></RFC>
            <curp></curp>
            <nombre></nombre>
            <primerApellido></primerApellido>
            <segundoApellido></segundoApellido>
        </personalFedatario>
        <DoRS>
            <Cud></Cud>
            <denominacionAutorizada></denominacionAutorizada>
            <regimenSocietario></regimenSocietario>
            <FechaAutorizacion></FechaAutorizacion>
        </DoRS>
        <RPC>
            <isCapitalVariable></isCapitalVariable>
            <instrumento>
                <tipoInstrumento></tipoInstrumento>
                <numero></numero>
                <volumen></volumen>
                <libro></libro>
                <fechaInstrumento></fechaInstrumento>
                <!--En caso de que se haya constituido por contrato privado-->
                <fechaDeConstitucion></fechaDeConstitucion>
            </instrumento>
            <duracionSociedad></duracionSociedad>
            <domicilioSociedad>
                <codigoPostal></codigoPostal>
                <entidadFederativa></entidadFederativa>
                <municipio></municipio>
                <localidad></localidad>
                <colonia></colonia>
                <calle></calle>
                <entreCalleYCalle></entreCalleYCalle>
                <numeroExterior></numeroExterior>
                <numeroInterior></numeroInterior>
            </domicilioSociedad>
            <objetoSocialPrincipal></objetoSocialPrincipal>
            <montoCapitalVariable></montoCapitalVariable>
            <montoCapitalFijo></montoCapitalFijo>
            <importeCapitalSocial></importeCapitalSocial>
            <tieneExpresionValorNominal></tieneExpresionValorNominal>
            <estructuraAccionaria>
                <!--Accionista o Representante Legal-->
                <personaFisica>
                    <rfcPF></rfcPF>
                    <curpPF></curpPF>
                    <nombrePF></nombrePF>
                    <primerApellidoPF></primerApellidoPF>
                    <segundoApellidoPF></segundoApellidoPF>
                    <fechaNacimientoPF></fechaNacimientoPF>
                    <nacionalidadPF></nacionalidadPF>
                    <domicilio>
                        <codigoPostal></codigoPostal>
                        <entidadFederativa></entidadFederativa>
                        <municipio></municipio>
                        <localidad></localidad>
                        <colonia></colonia>
                        <calle></calle>
                        <entreCalleYCalle></entreCalleYCalle>
                        <numeroExterior></numeroExterior>
                        <numeroInterior></numeroInterior>
                    </domicilio>
                    <Acciones>
                        <serie>
                            <tipoSerie></tipoSerie>
                            <numeroAcciones></numeroAcciones>
                            <valorDeLaAccion></valorDeLaAccion>
                            <totalCapital></totalCapital>
                            <participacionPF></participacionPF>
                        </serie>
                    </Acciones>
                </personaFisica>
                <personaMoral>
                    <rfcPM></rfcPM>
                    <DoRSAccionista></DoRSAccionista>
                    <fechaConstitucion></fechaConstitucion>
                    <nacionalidadPM></nacionalidadPM>
                    <domicilio>
                        <codigoPostal></codigoPostal>
                        <entidadFederativa></entidadFederativa>
                        <municipio></municipio>
                        <localidad></localidad>
                        <colonia></colonia>
                        <calle></calle>
                        <entreCalleYCalle></entreCalleYCalle>
                        <numeroExterior></numeroExterior>
                        <numeroInterior></numeroInterior>
                    </domicilio>
                    <Acciones>
                        <serie>
                            <tipoSeriePM></tipoSeriePM>
                            <numeroAccionesPM></numeroAccionesPM>
                            <valorDeLaAccionPM></valorDeLaAccionPM>
                            <totalCapitalPM></totalCapitalPM>
                            <participacionPM></participacionPM>
                        </serie>
                    </Acciones>
                </personaMoral>
            </estructuraAccionaria>
            <administracion>
                <isColegiada></isColegiada>
                <facultades></facultades>
                <nombramientos>
                    <tieneRfc></tieneRfc>
                    <rfc></rfc>
                    <curp></curp>
                    <nombre></nombre>
                    <primerApellido></primerApellido>
                    <segundoApellido></segundoApellido>
                    <nombramiento></nombramiento>
                </nombramientos>
            </administracion>
            <nombramientos>
                <rfcNom></rfcNom>
                <curpNom></curpNom>
                <nombreNom></nombreNom>
                <primerApellidoNom></primerApellidoNom>
                <segundoApellidoNom></segundoApellidoNom>
                <fechaNacimientoNom></fechaNacimientoNom>
                <nacionalidadNom></nacionalidadNom>
                <cargoNom></cargoNom>
                <PoderOtorgado>
                    <Facultades>
                        <consecutivo>?</consecutivo>
                        <tipoFacultad></tipoFacultad>
                        <comentarios></comentarios>
                    </Facultades>
                    <fechaOtorgamiento>?</fechaOtorgamiento>
                    <!--Optional-->
                    <fechaRevocacion>?</fechaRevocacion>
                </PoderOtorgado>
            </nombramientos>
            <apoderados>
                <rfcApo></rfcApo>
                <curpApo></curpApo>
                <nombreApo></nombreApo>
                <primerApellidoApo></primerApellidoApo>
                <segundoApellidoApo></segundoApellidoApo>
                <fechaNacimientoApo></fechaNacimientoApo>
                <nacionalidadApo></nacionalidadApo>
                <PoderOtorgado>
                    <Facultades>
                        <consecutivo>?</consecutivo>
                        <tipoFacultad></tipoFacultad>
                        <comentarios></comentarios>
                    </Facultades>
                    <fechaOtorgamiento>?</fechaOtorgamiento>
                    <!--Optional-->
                    <fechaRevocacion>?</fechaRevocacion>
                </PoderOtorgado>
            </apoderados>
            <!--Revisar integración del órganos de vigilancia-->
            <organoVigilancia></organoVigilancia>
            <otrasAutorizaciones></otrasAutorizaciones>
            <isConstitucionSucesiva></isConstitucionSucesiva>
            <!--Opcional-->
            <sociedadDerivada>
                <isFusion></isFusion>
                <isEscision></isEscision>
                <sociedades>
                    <folioMercantil></folioMercantil>
                    <denominacion></denominacion>
                    <domicilio>
                        <codigoPostal></codigoPostal>
                        <entidadFederativa></entidadFederativa>
                        <municipio></municipio>
                        <localidad></localidad>
                        <colonia></colonia>
                        <calle></calle>
                        <entreCalleYCalle></entreCalleYCalle>
                        <numeroExterior></numeroExterior>
                        <numeroInterior></numeroInterior>
                    </domicilio>
                </sociedades>
            </sociedadDerivada>
        </RPC>
        
    </AperturaExpedienteElectronico>
</xml>